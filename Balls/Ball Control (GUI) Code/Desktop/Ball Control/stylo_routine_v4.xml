<?xml version="1.0"?>
<routine name="Stylo Routine" number_balls="3" music="stylo.wav">
	
	<section time="0" name="Turn off balls" >
		<ball number="all">
				<effect type="solid" >
					<colour>0,0,0</colour>					
				</effect>	
		</ball>
				
	</section>
	

	

	<section time="652" name="Off to RGB" >
		<ball number="1">
				<effect type="fade" cycle="false">
					<delay>11</delay>
					<colour>0,0,0</colour>		
					<colour>255,0,0</colour>						
				</effect>
		</ball>
		
		<ball number="2">
				<effect type="fade" cycle="false">
					<delay>11</delay>
					<colour>0,0,0</colour>		
					<colour>0,255,0</colour>						
				</effect>
		</ball>
		
		<ball number="3">
				<effect type="fade" cycle="false">
					<delay>11</delay>
					<colour>0,0,0</colour>		
					<colour>0,0,255</colour>						
				</effect>
		</ball>			
	</section>



	<section time="7000" name="RGB to White" >
		<ball number="1">
				<effect type="fade" cycle="false">
					<delay>11</delay>
					<colour>255,0,0</colour>		
					<colour>255,255,255</colour>						
				</effect>
		</ball>
		
		<ball number="2">
				<effect type="fade" cycle="false">
					<delay>11</delay>
					<colour>0,255,0</colour>		
					<colour>255,255,255</colour>						
				</effect>
		</ball>
		
		<ball number="3">
				<effect type="fade" cycle="false">
					<delay>11</delay>
					<colour>0,0,255</colour>		
					<colour>255,255,255</colour>						
				</effect>
		</ball>			
	</section>
	
	

	
	
	
	
	
	
	
				<section time="15000" name="fade 2" >
		<ball number="all">
				<effect type="fade" cycle="false">
					<delay>11</delay>
					<colour>255,555,255</colour>		
					<colour>186,0,89</colour>						
				</effect>
		</ball>
		
</section>
	
	
	
	
	
	
	
	
							<section time="20000" name="fade 5" >
		<ball number="1">
				<effect type="fade" cycle="false">
					<delay>5</delay>
					<colour>186,0,89</colour>		
					<colour>255,0,0</colour>						
				</effect>
		</ball>
		
		<ball number="2">
				<effect type="fade" cycle="false">
					<delay>5</delay>
					<colour>186,0,89</colour>		
					<colour>0,255,0</colour>						
				</effect>
		</ball>
		
		<ball number="3">
				<effect type="fade" cycle="false">
					<delay>5</delay>
					<colour>186,0,89</colour>		
					<colour>0,0,255</colour>						
				</effect>
		</ball>	
	</section>
	
	
	
			<section name="All - Meteor Random " time="22000"  >
		<ball number="all">
				<effect type="meteor_random" >
					<colour>0,0,0</colour>
				
				</effect>	
		</ball>	
	</section>
	
	
	
	
										<section time="33000" name="throw random" >
		<ball number="all">
				<effect type="throw_random" ></effect>
		</ball>
		</section>
		
		
		<section time="36000" name="all balls off" >
			<ball number="all">
				<effect type="fade" cycle="false">
					<delay>10</delay>
					<colour>255,255,255</colour>		
					<colour>0,0,0</colour>						
				</effect>
		</ball>
	</section>
	
	
			<section time="39000" name="flashballs" >
			<ball number="all">
				<effect type="random_colours" >
					<delay>60</delay>
						
				</effect>
		</ball>
	</section>


	
</routine>